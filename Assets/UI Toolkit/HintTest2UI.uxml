        <ui:Label tabindex="-1" text="Excellent job! You spotted every suspicious element perfectly—impressive attention to detail!&#10;&#10;You’re getting the hang of this. Ready to take on the next challenge? Let’s keep pushing your skills to the next level!" parse-escape-sequences="true" display-tooltip-when-elided="true" name="successMsg" focusable="false" style="font-size: 28px; position: absolute; color: rgb(255, 255, 255); top: 279px; right: 420px; display: none; -unity-font-definition: url(&quot;project://database/Assets/Fonts/TechnoCharmDemoVersionRegular-ALaZm%20SDF.asset?fileID=11400000&amp;guid=7919249cbe97443598531dbbe1e5c973&amp;type=2#TechnoCharmDemoVersionRegular-ALaZm SDF&quot;); height: 283px; width: 738px; white-space: normal;" />
        <ui:VisualElement name="SlidesExplanation" style="flex-grow: 1; height: 428px; width: 908px; position: absolute; top: 269px; left: 744px;">
            <ui:Label tabindex="-1" text="Phrases like &quot;please verify your account immediately&quot; are designed to create panic." parse-escape-sequences="true" display-tooltip-when-elided="true" enable-rich-text="false" name="ClickItem1" style="color: rgb(255, 255, 255); font-size: 28px; height: 246px; width: 666px; white-space: normal; -unity-font-definition: url(&quot;project://database/Assets/Fonts/TechnoCharmDemoVersionRegular-ALaZm%20SDF.asset?fileID=11400000&amp;guid=7919249cbe97443598531dbbe1e5c973&amp;type=2#TechnoCharmDemoVersionRegular-ALaZm SDF&quot;); position: absolute; display: none;" />
            <ui:Label tabindex="-1" text="The &quot;Verify My Account Now&quot; link points to http://secure-payee.verify-login.net, which doesn’t match the sender email domain." parse-escape-sequences="true" display-tooltip-when-elided="true" enable-rich-text="false" name="ClickItem2" style="color: rgb(255, 255, 255); font-size: 28px; height: 306px; width: 726px; background-color: rgba(255, 255, 255, 0); white-space: normal; -unity-font-definition: url(&quot;project://database/Assets/Fonts/TechnoCharmDemoVersionRegular-ALaZm%20SDF.asset?fileID=11400000&amp;guid=7919249cbe97443598531dbbe1e5c973&amp;type=2#TechnoCharmDemoVersionRegular-ALaZm SDF&quot;); display: none;" />
            <ui:Label tabindex="-1" text="Phrases like &quot;If you do not verify your account within 24 hours, your account will be permanently suspended.&#10;&quot; are designed to create panic." parse-escape-sequences="true" display-tooltip-when-elided="true" enable-rich-text="false" name="ClickItem3" style="color: rgb(255, 255, 255); font-size: 28px; height: 246px; width: 666px; white-space: normal; -unity-font-definition: url(&quot;project://database/Assets/Fonts/TechnoCharmDemoVersionRegular-ALaZm%20SDF.asset?fileID=11400000&amp;guid=7919249cbe97443598531dbbe1e5c973&amp;type=2#TechnoCharmDemoVersionRegular-ALaZm SDF&quot;); position: absolute; display: none;" />
            <ui:Label tabindex="-1" text="Generic Greeting:&#10;* The email begins with &quot;Dear Valued Customer&quot; instead of addressing the recipient by name, which is unusual for official communications." parse-escape-sequences="true" display-tooltip-when-elided="true" enable-rich-text="false" name="ClickItem4" style="color: rgb(255, 255, 255); font-size: 28px; height: 246px; width: 666px; white-space: normal; -unity-font-definition: url(&quot;project://database/Assets/Fonts/TechnoCharmDemoVersionRegular-ALaZm%20SDF.asset?fileID=11400000&amp;guid=7919249cbe97443598531dbbe1e5c973&amp;type=2#TechnoCharmDemoVersionRegular-ALaZm SDF&quot;); position: absolute; display: none;" />
